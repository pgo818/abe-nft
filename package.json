{
  "name": "nft",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:nft": "npx hardhat test test/NFTContracts.test.js",
    "test:coverage": "npx hardhat coverage",
    "deploy:local": "npx hardhat ignition deploy ignition/modules/NFTContracts.js --network localhost",
    "deploy:sepolia": "npx hardhat ignition deploy ignition/modules/NFTContracts.js --network sepolia",
    "interact": "npx hardhat run scripts/interact.js --network localhost",
    "interact:hardhat": "npx hardhat run scripts/interact.js --network hardhat",
    "node": "npx hardhat node",
    "clean": "npx hardhat clean"
  },
  "keywords": [
    "NFT",
    "ERC721",
    "Solidity",
    "Hardhat"
  ],
  "author": "",
  "license": "MIT",
  "description": "NFT project with Hardhat framework",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^5.3.0",
    "hardhat": "^2.24.1"
  }
}